<div id="online-users-container" class="chatbox">
    <div class="chat-head oc-chat-head"> 
        <ul class="tabs">
            <li><a class="s" href="#users">Users</a></li>
            <li><a class="s" href="#chatrooms">Chatrooms</a></li>
        </ul>
    </div> 

    <!-- tab "panes" -->
    <div class="panes">
        <div id="users" class="oc-chat-content">
            <form class="set-xmpp-status" action="" method="post">
                <label class="status-select" for="select-xmpp-status"> 
                    My status is:
                    <select id="select-xmpp-status">
                        <option value="online">Online</option>
                        <option value="busy">Busy</option>
                        <option value="away">Away</option>
                        <option value="offline">Offline</option>
                    </select>
                </label>
            </form>
            <ul id="online-users"></ul>
        </div> 
        
        <div id="chatrooms" class="oc-chat-content">
            Here be chatrooms
        </div>
    </div>

</div>
