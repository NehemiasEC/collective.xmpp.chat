<div id="chatpanel" i18n:domain="collective.xmpp.chat"
    tal:condition="not: context/portal_membership/isAnonymousUser">
    <div id="collective-xmpp-chat-data"
        tal:attributes="
                fullname view/fullname;
                prebind python:True;
                auto_subscribe view/auto_subscribe;"></div>
    <div id="toggle-controlbox">
        <a href="#" class="chat" id="toggle-online-users">
            <span class="conn-feedback">Click here to chat</span> <strong style="display: none" id="online-count">(0)</strong>
        </a>
    </div>
</div>
